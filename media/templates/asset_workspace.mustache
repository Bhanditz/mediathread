    <td class="panhandle-left-container">
        <div class="panhandle-left asset-workspace"></div>
    </td>
    
    <td class="panhandle-stripe asset-workspace">
        <div class="label">{{panel_state_label}}</div>
    </td>

    <td id="asset-workspace-panel-container" class="panel-container {{panel_state}} asset-workspace parent">
        <div class="panel asset-workspace">
            <table class="panel-subcontainer">
                <tr class="asset-workspace-content-row">
                
                    <td class="panel-container open collection subpanel fluid">
                        <div id="media_gallery" class="media_gallery"></div>
                    </td>
                                    
                    <td class="panel-container open asset subpanel fixed" style="display: none">
                        {{#>>asset-view-header}}
                            <div id="asset-view-header">
                                {{#asset-current}}
                                     <div class="asset-view-title"><h1>{{title}}</h1></div>
                                {{/asset-current}}
                             
                                <input class="media newselection-button" type="button" onclick="javascript:AnnotationList.newAnnotation()" value="Create Selection"/>
                             </div>
                        {{/>>asset-view-header}}
                        
                        <div class="asset-view-container">
                            <div class="asset-view-published">
                                <div id="asset-workspace-videoclipbox" class="videoclipbox" style="display: none;">
                                    <div class="asset-object" style="border: none; background-color: #ededed;"></div>
                                    <div class="asset-display"></div>
                                    <div class="clipstrip-display"></div>
                                </div>
                            </div>
                            
                            {{#>>asset-view-details}}
                            <div id="asset-view-details" class="asset-view-details accordion" style="display: none">
                                <h3><a href="#">Item</a></h3>
                                <div>
                                    <p>
                                        {{#>>asset-global-annotation}}
                                            <div id="asset-global-annotation">
                                                {{#asset-current}}
                                                    <form id="edit-global-annotation-form" method="post" action="/asset/save/{{id}}/annotations/{{global_annotation_id}}/">
                                                        <div class="metadata">
                                                            <div class="metadata-label">My Tags:</div> 
                                                            <div class="metadata-value" onclick="AnnotationList.editItemField(this);">
                                                                {{#user_tags}}
                                                                    {{name}}{{^last}}, {{/last}}
                                                                {{/user_tags}}
                                                                {{^user_tags}}
                                                                    &lt;click to edit tags&gt;
                                                                {{/user_tags}}
                                                            </div>
                                                            <input type="text" name="annotation-tags" id="id_annotation-tags" 
                                                                value="{{#user_tags}}{{name}}{{^last}}, {{/last}}{{/user_tags}}"
                                                                onblur="AnnotationList.blurItemField(this)" style="display: none" />
                                                            <span style="display: none" class="helptext">Use commas to separate tags. Example: tag 1, tag 2, tag 3</span>
                                                            <input type="submit" name="submit_tags" value="Save" 
                                                                style="display: none" onclick="return AnnotationList.saveItem(this)" />
                                                            <div class="visualclear"></div>
                                                        </div>
                                                        <div class="metadata">
                                                            <div class="metadata-label">Notes:</div>
                                                            <div class="metadata-value" onclick="AnnotationList.editItemField(this);">
                                                                {{notes}}
                                                                {{^notes}}
                                                                    &lt;click to edit notes&gt;
                                                                {{/notes}}
                                                            </div>
                                                            <textarea style="display: none" id="id_annotation-body" rows="7" cols="51" 
                                                                onblur="AnnotationList.blurItemField(this)" 
                                                                name="annotation-body" value="{{notes}}">{{notes}}</textarea>
                                                            <input type="submit" name="submit_notes" value="Save" 
                                                                style="display: none" onclick="return AnnotationList.saveItem(this)" />
                                                        </div>
                                                        
                                                    </form>
                                                {{/asset-current}}
                                            </div>
                                         {{/>>asset-global-annotation}}
                                    </p>
                                </div>
                                
                                <h3><a href="#">Sources</a></h3>
                                <div>
                                    {{#>>asset-sources}}
                                        <div id="asset-sources">
                                            {{#asset-current}}
                                                <h4>Item's Original Source:</h4> 
                                                <a href="{{sources.url.url}}">{{sources.url.url?ellipses(50)}}</a>
                                                
                                                {{#sources.xmeml?}}
                                                    <h4 class="metadata-title">Final Cut Pro</h4>
                                                    <dl>
                                                        <dd>
                                                            <a onclick="return AnnotationList.showExportDialog(this, {{id}});" href="#">Export clips to XML</a>
                                                            <div class="export-finalcutpro" style="display: none" title="Export Final Cut Pro XML">
                                                                <div class="selection-list" title="Selections">
                                                                    <form name="export-finalcutpro" method="POST" action="/asset/xmeml/{{id}}/" 
                                                                        onsubmit="return AnnotationList.onExportSubmit();">
                                                                        <div>Order the selections by clicking and dragging. Click "Export" when you're ready.</div>
                                                                        <br />
                                                                        <ul class="selections"></ul>
                                                                    </form>    
                                                                </div>
                                                            </div>  
                                                        </dd>
                                                    </dl>    
                                                {{/sources.xmeml?}}
                                                
                                                {{#metadata}}
                                                    <h4>{{key}}</h4>
                                                    <div>{{value}}</div>
                                                {{/metadata}}
                                            {{/asset-current}}
                                        </div>
                                        
                                    {{/>>asset-sources}}
                                    <br />                           
                                </div>
                                
                                <h3><a href="#">References</a></h3>
                                <div>
                                    <p>
                                        {{#>>asset-references}}
                                            <div id="asset-references">
                                                {{#asset-current}}
                                                    {{#tags?}}
                                                        <h2>Class Tags</h2>
                                                        <div>
                                                            <span class="metadata-value">
                                                                {{#tags}}
                                                                    <a class="switcher-choice filterbyclasstag" 
                                                                        href="{{name}}">{{name}} ({{count}})</a> {{^last}}, {{/last}} 
                                                                {{/tags}}
                                                            </span>
                                                        </div>
                                                    {{/tags?}}
                                                    <br />
                                                    <br />
                                                    
                                                    {{#references?}}                                                
                                                        <h2>References</h2>
                                                        <ul class="clumper">
                                                        {{#references}}    
                                                            <li>
                                                                <div class="asset_title type-{{type}}">
                                                                    <a href="{{url}}" class="title">{{title}}</a>
                                                                </div>
                
                                                                <div class="asset-activities">
                   
                                                                    <div class="asset-activity">
                                                                        analyzed {{ modified }}
                                                                    </div>
                   
                                                                </div>
                                                            </li>                                            
                                                        {{/references}}
                                                        </ul>
                                                    {{/references?}}
                                                {{/asset-current}}
                                            </div>
                                        {{/>>asset-references}}
                                    </p>
                                </div>
                            </div> {{! end asset-view-details }}
                            {{/>>asset-view-details}}
                            
                            {{! Selections come next }}
                            
                        </div> {{! end asset-view-container }}
                    </td>
                </tr>
            </table>
        </div>
    </td>
    
    <td class="pantab-container">
        <div class="pantab asset-workspace {{panel_state}}"></div>
    </td>